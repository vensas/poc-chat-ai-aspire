  "version": "0.2.0",
  "configurations": [
    {
      "name": "Athena AppHost (HTTP)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Athena.AppHost/bin/Debug/net9.0/Athena.AppHost.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Athena.AppHost",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development",
        "ASPIRE_DASHBOARD_OTLP_ENDPOINT_URL": "http://localhost:19238",
        "ASPIRE_RESOURCE_SERVICE_ENDPOINT_URL": "http://localhost:20113",
        "ASPIRE_ALLOW_UNSECURED_TRANSPORT": "true"
      },
      "launchBrowser": {
        "enabled": true,
        "url": "http://localhost:19238"
      }
    },
    {
      "name": "Athena AppHost (HTTPS)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build",
      "program": "${workspaceFolder}/src/Athena.AppHost/bin/Debug/net9.0/Athena.AppHost.dll",
      "args": [],
      "cwd": "${workspaceFolder}/src/Athena.AppHost",
      "console": "internalConsole",
      "stopAtEntry": false,
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "DOTNET_ENVIRONMENT": "Development",
        "ASPIRE_DASHBOARD_OTLP_ENDPOINT_URL": "https://localhost:21268",
        "ASPIRE_RESOURCE_SERVICE_ENDPOINT_URL": "https://localhost:22266"
      }
    }
  ]
}
